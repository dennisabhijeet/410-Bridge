{
  "name": "410-brige-api",
  "version": "1.0.0",
  "description": "Restful server to handle request for 410 Bridge App",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "migrate": "node server/util/db/migrate",
    "migrate:test": "cross-env NODE_ENV=testing node server/util/db/migrate",
    "migrate:dev": "cross-env NODE_ENV=development node server/util/db/migrate",
    "seed:test": "npm run migrate:test && cross-env NODE_ENV=seed_test node server/util/db/index.js",
    "seed:dev": "npm run migrate:dev && cross-env NODE_ENV=seed_dev node server/util/db/index.js",
    "test": "npm run seed:test && jest",
    "test:watch": "jest --watch"
  },
  "author": "Mir Ayman ALi <alimirayman@outlook.com>",
  "engines": {
    "node": "18.17.1"
  },
  "dependencies": {
    "aws-sdk": "^2.680.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.18.3",
    "colors": "^1.3.1",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.4",
    "cron": "^1.8.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^7.7.7",
    "helmet": "^3.12.1",
    "html-to-text": "^5.1.1",
    "json2csv": "^4.2.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.10",
    "sequelize": "^6.28.2",
    "sequelize-cli": "^5.5.1",
    "socket.io": "^2.1.1",
    "umzug": "^2.3.0",
    "uuid": "^9.0.0",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "jest": "^29.6.4",
    "nodemon": "^2.0.18",
    "supertest": "^3.1.0"
  }
}
